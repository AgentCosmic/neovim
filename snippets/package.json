{
	"name": "snippets",
	"engines": {
		"vscode": "^1.11.0"
	},
	"contributes": {
		"snippets": [
			{
				"language": "solidity",
				"path": "./solidity.json"
			},
			{
				"language": "sql",
				"path": "./sql.json"
			},
			{
				"language": ["shellscript", "shell", "sh"],
				"path": "./shell.json"
			},
			{
				"language": ["markdown", "rmd"],
				"path": "./markdown.json"
			},
			{
