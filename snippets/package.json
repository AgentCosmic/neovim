			},
			{
				"language": "javascriptreact",
				"path": "./react.json"
			},
			{
				"language": "typescriptreact",
				"path": "./react-ts.json"
			},
			{
				"language": "go",
				"path": "./go.json"
			},
			{
				"language": "python",
